{
  "name": "pavelmaksimov25/jsonpath",
  "description": "Fork of the JSONPath implementation for parsing, searching and flattening arrays",
  "license": "MIT",
  "version": "0.2.1",
  "authors": [
    {
      "name": "Stephen Frank",
      "email": "stephen@flowsa.com",
      "homepage": "https://prismaticbytes.com",
      "role": "Developer"
    },
    {
      "name": "Sascha Greuel",
